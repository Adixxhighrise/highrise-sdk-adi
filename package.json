  },
  "homepage": "https://github.com/Adixxhighrise/highrise-sdk-adi#readme",
  "keywords": [
    "highrise",
    "highrise-sdk",
    "highrise-js",
    "bot",
    "chatbot",
    "sdk",
    "gaming",
    "api",
    "websocket",
    "fixed-version"
  ],
  "author": "Aditya Pratap Singh",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "colors": "^1.4.0",
    "events": "^3.3.0",
