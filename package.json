{
  "name": "highrise-sdk-adi",
  "version": "1.0.4",
  "description": "A JavaScript library for writing and running Highrise bots - Fixed version with bug fixes and improvements",
  "main": "./src/index.js",
  "types": "./index.d.ts",
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "files": [
    "src/**/*",
    "index.d.ts",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "node ./src/index.js",
    "test": "node -e \"console.log('Testing package...'); const pkg = require('./'); console.log('Success:', Object.keys(pkg));\"",
